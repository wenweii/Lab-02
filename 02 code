const byte LEDs[] = {11,10,9,8,5,4,3,2};
const byte total = sizeof(LEDs);
byte index = 0;
int i;
void setup()
{
    for (byte i=0;i<total;i++) 
    {
        pinMode(LEDs[i], OUTPUT);
    }
}

void loop() 
{
  for(byte i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
  }
  delay(1000);
  for(byte i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],LOW);
  }
   delay(1000);
 for(byte i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
  }
  delay(1000);
   for(byte i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],LOW);
  }
   delay(1000);
  for( i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
  for (i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
  for( i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
  for( i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
  for( i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
  for( i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
  for( i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
  for( i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
    for(byte i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
  }
  delay(1000);
  for(byte i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],LOW);
  }
   delay(1000);
 for(byte i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],HIGH);
  }
  delay(1000);
   for(byte i=0;i<8;i++)
  {
    digitalWrite( LEDs[i],LOW);
  }
   delay(1000);
    for(i=7;i>=0;i--)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
for(i=7;i>=0;i--)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
for(i=7;i>=0;i--)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
for(i=7;i>=0;i--)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
for(i=7;i>=0;i--)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
for(i=7;i>=0;i--)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
for(i=7;i>=0;i--)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
for(i=7;i>=0;i--)
  {
    digitalWrite( LEDs[i],HIGH);
    delay(100);
    digitalWrite( LEDs[i],LOW);
  }
delay(2000);
